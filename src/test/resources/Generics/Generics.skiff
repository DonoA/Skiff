class Wrapper<T> {
    dat: T;

    def Wrapper(dat: T) {
        this.dat = dat;
    }

    def getDat() -> T {
        return this.dat;
    }
}

def main(args: List<String>) -> Int {
    intData: Wrapper<Int>;
    intData = new Wrapper<Int>(5);

    dave: Wrapper<String>;
    dave = new Wrapper<String>("My name Dave!");

    println(dave.getDat());

    return intData.getDat();
}