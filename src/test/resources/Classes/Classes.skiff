class Thing {
    name: String;

    def Thing(name: String) {
        this.name = name;
    }

    def getName(): String {
        return name;
    }
}

class Person : Thing {
    age: Int;

    def Person(name: String, age: Int) {
        super(name);
        this.age = age;
    }

    def incAge() {
        this.age = age + 1;
    }

    def getAge(): Int {
        return this.age;
    }
}

def main(args: List<String>): Int {
    thing: Thing;
    thing = new Person("Dave", 15);
    println(thing.getName());
    dave: Person = thing;
    dave.incAge();
    return dave.getAge();
}