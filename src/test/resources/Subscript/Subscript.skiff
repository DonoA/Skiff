class Container {
    def Container() { }

    def getSub(Int i) -> Int {
        print("Getting item at ");
        println(i.toString());
        return 1;
    }

    def assignSub(Int elt, Int i) {
        print("Placing ");
        print(elt.toString());
        print(" at index ");
        println(i.toString());
    }
}

def main(List<String> args) -> Int {
    println(args[0]);
    args[0] = "New String";
    println(args[0]);

    let Container c = new Container();
    c[5] = 15;
    return c[1];
}