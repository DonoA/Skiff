class Container {
    def Container() { }

    def getSub(i: Int) -> Int {
        print("Getting item at ");
        println(i.toString());
        return 1;
    }

    def assignSub(elt: Int, i: Int) {
        print("Placing ");
        print(elt.toString());
        print(" at index ");
        println(i.toString());
    }
}

def main(args: List<String>) -> Int {
    println(args[0]);
    args[0] = "New String";
    println(args[0]);

    c: Container = new Container();
    c[5] = 15;
    return c[1];
}