enum Job {
	BUILDER,
	DRIVER,
	MINER;
}

class Person {
	private name: String;
	private age: Int;
	private id: Int;
	@Getter @Setter
	private job: Job;
	@Setter
	private static idCounter: Int;

	def Person(name: String, age: Int) {
		this.name = name;
		this.age = age;
		this.id = Person.idCounter;
		Person.idCounter++;
	}

	@Override
	def setJob(new_job: Job): None {
		this.job = new_job;
	}

	def getId(): Int {
		return id;
	}

	def sayHello(): None {
		print(name, "says hello");
	}

	@Operator(Operators.INCREMENT)
	def tickAge(): None {
		age++;
	}
}

person: Person = new Person("David", 35);

