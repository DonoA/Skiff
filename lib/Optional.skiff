class Optional<T> {
    private data: T;

    def Optional(data: T) {
        this.data = data;
    }

    static def of<U>(data: U) -> Optional<U> {
        return new Optional<U>(data);
    }

    static def empty<U>() -> Optional<U> {
        return new Optional<U>(null);
    }

    def get() -> T {
        return data;
    }

    def isPresent() -> Bool {
        return data != null;
    }
}